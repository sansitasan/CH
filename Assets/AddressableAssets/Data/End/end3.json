    {
      "talkname": [
        "<color=#D1F1FF>아라하시 타비"
      ],
      "discription": [
        1,
        "1"
      ],
      "data": {
        "state": "5",
        "dialog": "방금 뭔가 불길한 소리가 들리지 않았어?"
      }
    },
    {
      "talkname": [
        "<color=#D1F1FF>아라하시 타비"
      ],
      "discription": [
        1,
        "1"
      ],
      "data": {
        "state": "5",
        "dialog": "어라?"
